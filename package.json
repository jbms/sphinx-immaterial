{
  "name": "sphinx-immaterial",
  "version": "0.0.0",
  "description": "Sphinx Material design theme",
  "keywords": [
    "sphinx",
    "documentation",
    "theme"
  ],
  "homepage": "https://github.com/jbms/sphinx-immaterial/",
  "bugs": {
    "url": "https://github.com/jbms/sphinx-immaterial/issues",
    "email": "jeremy@jeremyms.com"
  },
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Jeremy Maitin-Shepard",
    "email": "jeremy@jeremyms.com"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/jbms/sphinx-immaterial.git"
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "build": "npm run clean && ts-node -T tools/build --optimize",
    "build:dev": "npm run clean && ts-node -T tools/build",
    "build:dirty": "ts-node -T tools/build --dirty",
    "build:noclean": "ts-node -T tools/build --verbose",
    "docs": "npm run build && rimraf docs/_build && cd docs && sphinx-build . _build -a",
    "docs:dev": "npm run build:dev && rimraf docs/_build/* && cd docs && sphinx-build . _build -a",
    "clean": "rimraf sphinx_immaterial/{*.html,partials,.icons,static}",
    "check": "run-p check:*",
    "check:build": "tsc --noEmit",
    "check:style": "run-p check:style:*",
    "check:style:scss": "stylelint \"src/**/*.scss\"",
    "check:style:ts": "eslint --cache \"src/**/*.ts\"",
    "start": "ts-node -T tools/build --verbose --dirty --watch",
    "upgrade": "run-s upgrade:*",
    "upgrade:bump": "ncu --upgrade --filterVersion \"/^\\^/\"",
    "upgrade:install": "npm install"
  },
  "dependencies": {
    "array-flat-polyfill": "^1.0.1",
    "clipboard": "^2.0.10",
    "escape-html": "^1.0.3",
    "focus-visible": "^5.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.5.4",
    "unfetch": "^4.2.0",
    "url-polyfill": "^1.1.12"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@mdi/svg": "^6.5.95",
    "@primer/octicons": "^16.3.1",
    "@types/clipboard": "^2.0.7",
    "@types/css-modules": "^1.0.2",
    "@types/escape-html": "1.0.1",
    "@types/html-minifier": "^4.0.2",
    "@types/node": "^17.0.21",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "autoprefixer": "^10.4.2",
    "chokidar": "^3.5.3",
    "cssnano": "^5.0.17",
    "esbuild": "^0.14.23",
    "eslint": "^8.10.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-no-null": "^1.0.2",
    "github-types": "^1.0.0",
    "gitlab": "^14.2.2",
    "google-fonts-complete": "^2.1.1",
    "html-minifier": "^4.0.0",
    "material-design-color": "^2.3.2",
    "material-shadows": "^3.0.1",
    "npm-check-updates": "^12.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "postcss-dir-pseudo-class": "^6.0.4",
    "postcss-inline-svg": "^5.0.0",
    "postcss-logical": "^5.0.4",
    "preact": "^10.6.6",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.1.0",
    "svgo": "2.3.1",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">= 16"
  }
}
