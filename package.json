{
  "name": "sphinx-immaterial",
  "version": "0.0.0",
  "description": "Sphinx Material design theme",
  "keywords": ["sphinx", "documentation", "theme"],
  "homepage": "https://github.com/jbms/sphinx-immaterial/",
  "bugs": {
    "url": "https://github.com/jbms/sphinx-immaterial/issues",
    "email": "jeremy@jeremyms.com"
  },
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Jeremy Maitin-Shepard",
    "email": "jeremy@jeremyms.com"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/jbms/sphinx-immaterial.git"
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "build": "npm run clean && ts-node -T tools/build --optimize",
    "build:dev": "npm run clean && ts-node -T tools/build",
    "build:dirty": "ts-node -T tools/build --dirty",
    "build:noclean": "ts-node -T tools/build --verbose",
    "docs": "npm run build && rimraf docs/_build && cd docs && sphinx-build . _build -a",
    "docs:dev": "npm run build:dev && rimraf docs/_build/* && cd docs && sphinx-build . _build -a",
    "docs:watch": "ts-node -T tools/build --verbose --dirty --watch --docs",
    "clean": "rimraf --glob sphinx_immaterial/{*.html,partials,.icons,LICENSE,bundles}",
    "check": "run-p check:*",
    "check:build": "tsc --noEmit",
    "check:style": "run-p check:style:*",
    "check:style:scss": "stylelint \"src/**/*.scss\"",
    "check:style:ts": "eslint --cache \"src/**/*.ts\" \"tools/**/*.ts\"",
    "fix": "run-p fix:*",
    "fix:style": "run-p fix:style:*",
    "fix:style:ts": "eslint --cache \"src/**/*.ts\" \"tools/**/*.ts\" --fix",
    "start": "ts-node -T tools/build --verbose --all --dirty --watch",
    "upgrade": "run-s upgrade:*",
    "upgrade:bump": "ncu --upgrade --filterVersion \"/^\\^/\"",
    "upgrade:install": "npm install"
  },
  "dependencies": {
    "clipboard": "^2.0.11",
    "escape-html": "^1.0.3",
    "focus-visible": "^5.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mdi/svg": "^7.4.47",
    "@primer/octicons": "^19.15.0",
    "@stylistic/eslint-plugin-ts": "^3.1.0",
    "@stylistic/stylelint-config": "^2.0.0",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "@types/css-modules": "^1.0.5",
    "@types/escape-html": "^1.0.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/node": "^22.13.1",
    "@types/resize-observer-browser": "^0.1.11",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "autoprefixer": "^10.4.20",
    "chokidar": "^4.0.3",
    "cssnano": "5.1.0",
    "esbuild": "^0.25.0",
    "eslint": "8.57.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-no-null": "^1.0.2",
    "github-types": "^1.0.0",
    "gitlab": "^14.2.2",
    "google-fonts-complete": "jonathantneal/google-fonts-complete",
    "html-minifier-terser": "^7.2.0",
    "material-design-color": "^2.3.2",
    "material-shadows": "^3.0.1",
    "mathjax": "^3.2.2",
    "mermaid": "^10.7.0",
    "npm-check-updates": "^17.1.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.2",
    "postcss-dir-pseudo-class": "^9.0.1",
    "postcss-inline-svg": "^6.0.0",
    "postcss-logical": "5.0.4",
    "postcss-pseudo-is": "^0.3.0",
    "preact": "^10.25.4",
    "rimraf": "^6.0.1",
    "sass": "^1.84.0",
    "simple-icons": "^14.6.0",
    "stylelint": "16.12.0",
    "stylelint-config-recess-order": "5.1.1",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-scss": "^6.11.0",
    "svgo": "^3.3.2",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">= 18"
  }
}
